{
  "name": "hd-assay",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "api": "cd swapi && npm run server && cd -",
    "client": "ng serve",
    "start": "concurrently \"npm:api\" \"npm:client\"",
